{
  "name": "jest-coverage-badges-ng",
  "version": "0.0.1",
  "description": "Create a group of coverage badges from jest",
  "main": "cli.js",
  "bin": "cli.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelrodov/jest-coverage-badges-ng.git"
  },
  "keywords": [
    "coverage",
    "badge",
    "jest",
    "istanbul"
  ],
  "author": "Michael Rodov <rodov.michael@gmail.com>",
  "engines": {
    "node": ">=6.11",
    "npm": ">=5.3"
  },
  "bugs": {
    "url": "https://github.com/michaelrodov/jest-coverage-badges-ng/issues"
  },
  "homepage": "https://github.com/michaelrodov/jest-coverage-badges-ng/blob/master/README.md",
  "scripts": {
    "lint": "eslint --fix -- .",
    "test": "jest --coverage"
  },
  "dependencies": {
    "mkdirp": "0.5.1"
  },
  "devDependencies": {
    "@types/jest": "29.5.0",
    "eslint": "8.36.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-quintoandar-base": "1.0.1",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.11.1",
    "jest": "29.5.0",
    "jest-coverage-badges-ng": "0.0.1",
    "jest-junit": "15.0.0",
    "semantic-release": "^6.3.6",
    "standard": "10.0.2",
    "validate-commit-msg": "2.12.1"
  },
  "files": [
    "cli.js"
  ],
  "license": "MIT",
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 65
      }
    },
    "collectCoverageFrom": [
      "./src/components/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      ".*\\.s?css",
      ".*\\.test\\.js",
      ".*\\.stories\\.js",
      ".*\\.lazy\\.js"
    ]
  }
}
